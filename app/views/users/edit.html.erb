<h1> Edit User</h1>

<%= form_with model: @user do |f| %>

    <div class="field">
      <%= f.label :username %><br />
      <%= f.text_field :username, autofocus: true %>
    </div>
  
    <div class="field">
      <%= f.label :email %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
    </div>

    <div>  
      <%= f.submit %>
    </div>

<% end %>
