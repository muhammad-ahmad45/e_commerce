<h1>All Users</h1>
<br>
<ul>
  <% @users.each do |user| %>
    <li>
      <%= user.username %>
      <%= link_to 'Edit' , edit_user_path(user) %>
      <%= button_to 'Delete' ,user_path(user), method: :delete%>
    </li>
    <% end %>
</ul>
<%= button_to "Sign out", destroy_user_session_path, method: :delete %>